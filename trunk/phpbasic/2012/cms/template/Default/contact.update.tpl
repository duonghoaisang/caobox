<div class="breadcrumb">{xpath}</div>
<div class="table_list toolbar"><a href="{http_referer}">Back</a></div>
<div class="form"><form action="" method="post" enctype="multipart/form-data" name="form1">
  <table width="100%" border="0" cellspacing="0" cellpadding="1" class="table_list">
    <tr>
      <td width="16%">Name</td>
      <td width="84%"><input name="name" type="text" id="name" value="{name}"></td>
    </tr>
    <tr>
      <td>Address</td>
      <td><input name="address" type="text" id="address" value="{address}"></td>
    </tr>
    <tr>
      <td>Email</td>
      <td><input name="email" type="text" id="email" value="{email}"></td>
    </tr>
    <tr>
      <td>Tel</td>
      <td><input name="tel" type="text" id="tel" value="{tel}"></td>
    </tr>
    <tr>
      <td>Fax</td>
      <td><input name="fax" type="text" id="fax" value="{fax}"></td>
    </tr>
  </table>
  <div class="table_list" align="right">
    <input type="submit" class="btn" name="Submit" value="&nbsp;">
  </div>
</form></div>
